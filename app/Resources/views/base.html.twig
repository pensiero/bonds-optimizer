<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Bonds Optimizer{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}" />
        {% stylesheets filter="cssrewrite,scssphp" output="css/app.css"
            "@AppBundle/Resources/scss/main.scss"
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}

        {% javascripts
            'assets/vendor/jquery/dist/jquery.min.js'
            'assets/vendor/floatThead/dist/jquery.floatThead.js'
            '@AppBundle/Resources/js/vendor/jquery.graphup.js'
            '@AppBundle/Resources/js/main.js'
        %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
